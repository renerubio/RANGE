{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown", "better-docs/component"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "better-docs": {
      "name": "Range Component"
    }
  },
  "opts": {
    "recurse": true,
    "destination": "docs",
    "readme": "docs/README.md"
  }
}
